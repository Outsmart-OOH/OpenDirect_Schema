{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/Outsmart-OOH/OpenDirect_Schema/main/v1.5.1/uris/organizations/organizations_collection_response.json",
    "type": "object",
    "title": "Response for a collection of Organizations",
    "description": "The schema of a response that gives a collection of Organizations",
    "required": [
        "Organizations"
    ],
    "additionalProperties": false,
    "properties": {
        "$schema": {
            "type": "string"
        },
        "Organizations": {
            "$id": "#/properties/organizations",
            "type": "array",
            "title": "Array of Organizations",
            "description": "Collection of Organizations",
            "items": {
                "$ref": "https://raw.githubusercontent.com/Outsmart-OOH/OpenDirect_Schema/main/v1.5.1/resources/organization/organization_object.json",
                "required": [
                    "Id",
                    "Name"
                ]
            }
        }
    }
}